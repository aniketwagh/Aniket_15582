package com.capston.shopping.model;

public class ProductsList {

	private static final ProductsDetails [] info = new ProductsDetails[5];
	
	static {
		info[0]=new ProductsDetails("Iphone", 45000);
		info[1]=new ProductsDetails("realme", 25999);
		info[2]=new ProductsDetails("Mi", 12000);
		info[3]=new ProductsDetails("Samsung", 43000);
		info[4]=new ProductsDetails("nokia", 41000);
	}

	public ProductsList() {
		super();
	}

	public static ProductsDetails[] getInfo() {
		return info;
	}
	
}
