public class ProductsDetails {

	public static void main(String[] args) {
		
		ProductCollection pc = ProductCollection.getCollection();
		Set<Products> p = pc.getList();
		p.add(new Products("nokia"));
		p.add(new Products("nokia1"));
		p.add(new Products("nokia3"));
		for(Products p1 : p)
		{
			System.out.println(p1);
		}
//		final SavingAccount account1=collection.findByAccNo(3);
		
		System.out.println(pc.findById(2));
	}
}
