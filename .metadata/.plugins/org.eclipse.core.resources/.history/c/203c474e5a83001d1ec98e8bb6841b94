package com.ecom.main;

import java.util.Set;

import com.ecom.dao.CustomerCollection;

import com.ecom.model.Customer;


public class CustomerDetails {

	public static void main(String[] args) {
		CustomerCollection pc = CustomerCollection.getCollection();
		Set<Customer> c = pc.getList();
		System.out.println("!================Disply all Records================!");
//		custName, String custEmail, double walletBalance, long phoneNo
		c.add(Customer("ANIKET"));
		c.add(Customer("ANIKET WAGHMARE","waghmareaniket7@gmail.com", 45345 ,70377355));
		for(Customer c1 : c)
		{
			System.out.println(c1);
		}	// TODO Auto-generated method stub

	}

}
